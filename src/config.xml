<?xml version='1.0' encoding='UTF-8'?>
<widget xmlns="http://wirecloud.conwet.fi.upm.es/ns/macdescription/1" vendor="CoNWeT" name="bae-browser" version="0.1.0">
  <details>
    <title>BAE Browser</title>
    <homepage>https://github.com/Wirecloud/bae-browser-widget</homepage>
    <authors>Jaime Pajuelo</authors>
    <email>jpajuelo@conwet.com</email>
    <image></image>
    <smartphoneimage></smartphoneimage>
    <description>The best project ever.</description>
    <longdescription>DESCRIPTION.md</longdescription>
    <changelog>doc/changelog.md</changelog>
    <doc>doc/index.md</doc>
    <license>Apache-2.0</license>
    <issuetracker>https://github.com/Wirecloud/bae-browser-widget/issues</issuetracker>
  </details>
  <requirements></requirements>
  <preferences>
    <preference name="server_url" type="text" label="Server URL" description="URL of the BAE server."/>
  </preferences>
  <wiring>
    <inputendpoint name="query" type="text" label="Query" description="String. One or more space-separated keywords." friendcode="query"/>
    <inputendpoint name="filters" type="text" label="Filters" description="JSON String. Search filters selected." friendcode="filters"/>
  </wiring>
  <contents src="index.html"/>
  <rendering height="300px" width="30%"/>
</widget>